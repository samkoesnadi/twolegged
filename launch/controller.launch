<launch>

<rosparam file="$(find robot_to_export_description)/launch/controller.yaml" command="load"/>
<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="robot_to_export" args="hip_pitch_position_controller hip_roll_position_controller knee_roll_position_controller joint_state_controller "/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
  <remap from="/joint_states" to="/robot_to_export/joint_states"/>
</node>

</launch>